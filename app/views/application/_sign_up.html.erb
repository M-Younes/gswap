<h2>Sign up</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <div class="form-group">
    <%= f.email_field :email, :required => true, class: "form-control", :placeholder => "Email address" %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, :required => true, class: "form-control", :placeholder => "password" %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, :required => true, class: "form-control", :placeholder => "password confirmation" %>
  </div>
  <div class="modal-footer">
    <%= f.submit "Sign up", class: "btn btn-primary center-block" do %>
  <% end %>
</div>
<% end %>


